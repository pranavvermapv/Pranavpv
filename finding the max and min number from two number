#include<stdio.h>

void main()
{
    int a, b;
    
    printf("enter the first number: ");
    scanf("%d",&a);
     printf("enter the second number: ");
    scanf("%d",&b);
    
    if(a>b)
    {
    printf("%d is maximum number".&a)
    }
    if(a<b)
    {
        printf("%d is the minimum number"
    }
    
    
    return 0;
}
